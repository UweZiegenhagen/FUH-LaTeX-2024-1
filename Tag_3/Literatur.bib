@Article{Sparavigna2013,
  author       = {Sparavigna, Amelia Carolina},
  date         = {2013-08-24},
  journaltitle = {International Journal of Sciences, 2013, 2(8):102-104},
  title        = {Edge-detection applied to moving sand dunes on Mars},
  doi          = {10.18483/ijsci.251},
  eprint       = {1308.5315},
  eprintclass  = {cs.CV},
  eprinttype   = {arXiv},
  issn         = {2410-4477},
  number       = {08},
  pages        = {102--104},
  volume       = {1},
  abstract     = {Here we discuss the application of an edge detection filter, the Sobel filter of GIMP, to the recently discovered motion of some sand dunes on Mars. The filter allows a good comparison of an image HiRISE of 2007 and an image of 1999 recorded by the Mars Global Surveyor of the dunes in the Nili Patera caldera, measuring therefore the motion of the dunes on a longer period of time than that previously investigated.},
  copyright    = {arXiv.org perpetual, non-exclusive license},
  file         = {:Carolina_Sparavigna_2013 - Edge Detection Applied to Moving Sand Dunes on Mars.pdf:PDF},
  keywords     = {Computer Vision and Pattern Recognition (cs.CV), FOS: Computer and information sciences},
  publisher    = {Alkhaer Publications},
  year         = {2013},
}

@Article{Cao2021,
  author       = {Cao, Jacky and Lam, Kit-Yung and Lee, Lik-Hang and Liu, Xiaoli and Hui, Pan and Su, Xiang},
  date         = {2021-06-16},
  journaltitle = {ACM Computing Surveys},
  title        = {Mobile Augmented Reality: User Interfaces, Frameworks, and Intelligence},
  doi          = {10.1145/3557999},
  eprint       = {2106.08710},
  eprintclass  = {cs.HC},
  eprinttype   = {arXiv},
  issn         = {1557-7341},
  number       = {9},
  pages        = {1--36},
  volume       = {55},
  abstract     = {Mobile Augmented Reality (MAR) integrates computer-generated virtual objects with physical environments for mobile devices. MAR systems enable users to interact with MAR devices, such as smartphones and head-worn wearables, and performs seamless transitions from the physical world to a mixed world with digital entities. These MAR systems support user experiences by using MAR devices to provide universal accessibility to digital contents. Over the past 20 years, a number of MAR systems have been developed, however, the studies and design of MAR frameworks have not yet been systematically reviewed from the perspective of user-centric design. This article presents the first effort of surveying existing MAR frameworks (count: 37) and further discusses the latest studies on MAR through a top-down approach: 1) MAR applications; 2) MAR visualisation techniques adaptive to user mobility and contexts; 3) systematic evaluation of MAR frameworks including supported platforms and corresponding features such as tracking, feature extraction plus sensing capabilities; and 4) underlying machine learning approaches supporting intelligent operations within MAR systems. Finally, we summarise the development of emerging research fields, current state-of-the-art, and discuss the important open challenges and possible theoretical and technical directions. This survey aims to benefit both researchers and MAR system developers alike.},
  copyright    = {arXiv.org perpetual, non-exclusive license},
  file         = {:Cao2021 - Mobile Augmented Reality_ User Interfaces, Frameworks, and Intelligence.pdf:PDF},
  keywords     = {Human-Computer Interaction (cs.HC), Artificial Intelligence (cs.AI), Computer Vision and Pattern Recognition (cs.CV), Computers and Society (cs.CY), Machine Learning (cs.LG), FOS: Computer and information sciences},
  month        = jan,
  publisher    = {Association for Computing Machinery (ACM)},
  year         = {2021},
}

